{
  "name": "nextjs-testing-library",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --env=jsdom --verbose",
    "lint": "next lint"
  },
  "dependencies": {
    "autoprefixer": "^10.4.2",
    "axios": "^0.21.1",
    "msw": "^0.35.0",
    "next": "11.1.2",
    "postcss": "^8.4.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.2.1",
    "tailwindcss": "^3.0.19"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.39",
    "babel-jest": "^27.5.1",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "jest": "^27.5.1",
    "jest-css-modules": "^2.1.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
